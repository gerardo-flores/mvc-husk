<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Public/css/Site.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Public/css/ui-lightness/jquery-ui-1.8.13.custom.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Public/css/ui.jqgrid.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Public/css/jquery.jqplot.min.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Public/javascripts/jquery-1.6.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Public/javascripts/modernizr-1.7.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Public/javascripts/i18n/grid.locale-en.js")" type="text/javascript" ></script>
    <script src="@Url.Content("~/Public/javascripts/jquery.jqGrid.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Public/javascripts/jquery.jqPlot.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Public/javascripts/jquery-ui-1.8.13.min.js")" type="text/javascript" ></script>
    <!--[if lt IE 9]><script src="@Url.Content("~/Public/javascripts/excanvas.min.js")" type="text/javascript" ></script><![endif]-->
    @RenderSection("pageJS", false)
    @MvcMiniProfiler.MiniProfiler.RenderIncludes() 
</head>
<body>
    <div class="page">
        <header>
            <div id="title">
                <h1>.Net MVC Shell</h1>
            </div>
            <div id="logindisplay">
                @Html.Partial("_LogOnPartial")
            </div>
            @Html.Partial("_NavigationPartial")
        </header>
        <section id="main">
            @RenderBody()
        </section>
        <footer>
        </footer>
    </div>
</body>
</html>
